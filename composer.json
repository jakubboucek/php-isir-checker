{
  "name": "asisteam/isir",
  "description": "Implemenation of communication with ISIR API (Insolvency registry API by Ministerstvo spravedlnosti CR)",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "ISIR",
    "insolvency",
    "API"
  ],
  "authors": [
    {
      "name": "Tomas Sedlacek",
      "homepage": "http://kedlas.cz"
    }
  ],
  "homepage": "https://github.com/AsisTeam/isir",
  "require": {
    "php": "^8.3",
    "ext-soap": "*"
  },
  "require-dev": {
    "ext-simplexml": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "AsisTeam\\ISIR\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AsisTeam\\ISIR\\Tests\\": "tests"
    }
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "tests": [
      "tester -s -p php --colors 1 -C tests/Cases"
    ]
  }
}
